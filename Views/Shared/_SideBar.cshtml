<ejs-sidebar id="sidebar" width="250" enableDock dockSize="75px" target=".dock-target" enableGestures="false">
    <e-content-template>
        <div class="container">
            <i id="sidebar-toggler" class="bi bi-arrow-left-right"></i>
        </div>
        <div id="menu-container">
            <ul class="nav flex-column" id="menu-list">
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index"><i class="bi bi-house mx-2"></i>Home</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="JournalEntry" asp-action="Index"><i class="bi bi-book-half mx-2"></i>Journal</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="Notes" asp-action="Index"> <i class="bi bi-stickies mx-2"></i>Notes</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="MoodRating" asp-action="Dashboard">Tracker</a>
                </li>
            </ul>
            </ul>
        </div>

        <div>
            <partial name="_LoginPartial" />
        </div>
    </e-content-template>
</ejs-sidebar>

<script>
    document.addEventListener('DOMContentLoaded', function () {
        dockBar = document.getElementById('sidebar').ej2_instances[0];
        document.getElementById('sidebar-toggler').onclick = function () {
            dockBar.toggle();
        };
    });
</script>